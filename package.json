{
  "name": "openapi-to-http",
  "displayName": "OpenAPI to HTTP",
  "description": "Generate .http files from OpenAPI specifications for API testing",
  "version": "0.1.1",
  "publisher": "alexermolov",
  "author": "Alex Ermolov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexermolov/openapi-to-http"
  },
  "bugs": {
    "url": "https://github.com/alexermolov/openapi-to-http/issues"
  },
  "homepage": "https://github.com/alexermolov/openapi-to-http#readme",
  "keywords": [
    "openapi",
    "swagger",
    "http",
    "rest",
    "api",
    "testing",
    "rest-client",
    "api-testing",
    "openapi3"
  ],
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Testing",
    "Programming Languages"
  ],
  "activationEvents": [
    "onCommand:openapi-to-http.generateHttp"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "openapi-to-http.generateHttp",
        "title": "OpenAPI: Generate .http file"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "openapi-to-http.generateHttp"
        }
      ],
      "editor/context": [
        {
          "command": "openapi-to-http.generateHttp",
          "group": "1_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^20.x",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^8.40.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "json-schema-faker": "^0.5.6",
    "openapi-types": "^12.1.3"
  }
}
